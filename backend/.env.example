# Google Gemini API Key - REQUIRED for SynthID detection
# Get yours from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Flask Server Settings
PORT=5000
FLASK_ENV=development
DEBUG=True

